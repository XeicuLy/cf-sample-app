{
  "name": "backend",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "deploy:preview": "wrangler deploy --minify --env=preview",
    "deploy:production": "wrangler deploy --minify --env=production",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "typecheck": "pnpm run cf-typegen && tsc --noEmit"
  },
  "dependencies": {
    "hono": "4.10.7"
  },
  "devDependencies": {
    "wrangler": "4.51.0"
  }
}
